{
  "expo": {
    "name": "react-native-guess-my-number",
    "slug": "react-native-guess-my-number",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4E0329"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amcodebytes.reactnativeguessmynumber",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4E0329"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.amcodebytes.reactnativeguessmynumber"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "00a8280e-d53e-49f7-b2e5-7dcd50e6b3d4"
      }
    }
  }
}
